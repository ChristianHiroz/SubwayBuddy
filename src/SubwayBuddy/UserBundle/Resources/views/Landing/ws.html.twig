{% extends "::base.html.twig" %}
{% block body %}
    <div class="modal fade" id="userModal" tabindex="-1" role="dialog">
      <div class="modal-dialog" style="width:80%;">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title">User API</h4>
          </div>
          <div class="modal-body">
              <table class="table table-condensed">
               <tr>
                  <th>Nom</th>
                  <th>Méthode</th>
                  <th>Url</th>
                  <th>Paramètre</th>
                  <th>Retour</th>
              </tr>
              <tr class="active">
                    <td>User registration</td>
                    <td>POST</td>
                    <td>/api/users</td>
                    <td>username (String) email (String) password (String)</td>
                    <td>Code 200: user (User)</td>
                </tr>
                <tr class="danger">
                    <td>User login</td>
                    <td>POST</td>
                    <td>/api/users/login</td>
                    <td>username(String) password (String)</td>
                    <td>Code 200: user (User)</td>
                </tr>
                <tr class="success">
                    <td>User update</td>
                    <td>PUT</td>
                    <td>/api/users</td>
                    <td>username(String) email (String) password (String) user (int : userId)</td>
                    <td>Code 200: user (User)</td>
                </tr>
              </table>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
          </div>
        </div><!-- /.modal-content -->
      </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
    <div class="modal fade" id="travelModal" tabindex="-1" role="dialog">
      <div class="modal-dialog" style="width:80%;">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title">Travel API</h4>
          </div>
          <div class="modal-body">
              <table class="table table-condensed">
               <tr>
                  <th>Nom</th>
                  <th>Méthode</th>
                  <th>Url</th>
                  <th>Paramètre</th>
                  <th>Retour</th>
              </tr>
              <tr class="active">
                    <td>Travel creation</td>
                    <td>POST</td>
                    <td>/api/travels</td>
                    <td>time (DateTime format 2012-07-08 11:14:15) name (String) user (int : userId)</td>
                    <td>Code 200: travel (Travel)</td>
                </tr>
                <tr class="danger">
                    <td>Travel list</td>
                    <td>GET</td>
                    <td>/api/travels</td>
                    <td>None</td>
                    <td>Code 200: travels (list Travel)</td>
                </tr>
                <tr class="success">
                    <td>Travel select</td>
                    <td>GET</td>
                    <td>/api/travels/[travelId}</td>
                    <td>travelId (int : travelId)</td>
                    <td>Code 200: travel (Travel)</td>
                </tr>
                <tr class="info">
                    <td>Travel list by User</td>
                    <td>GET</td>
                    <td>/api/user/{userId}travels/</td>
                    <td>userId (int : userId)</td>
                    <td>Code 200: travels (list Travel)</td>
                </tr>
                <tr class="active">
                    <td>Travel update</td>
                    <td>POST</td>
                    <td>/api/travels</td>
                    <td>name (String) time (DateTime format 2012-07-08 11:14:15) user (int : userId) travel (int : travelId)</td>
                    <td>Code 200: travel (Travel)</td>
                </tr>
              </table>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
          </div>
        </div><!-- /.modal-content -->
      </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
    <div class="modal fade" id="subjectModal" tabindex="-1" role="dialog">
      <div class="modal-dialog" style="width:80%;">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title">Subject API</h4>
          </div>
          <div class="modal-body">
              <table class="table table-condensed">
               <tr>
                  <th>Nom</th>
                  <th>Méthode</th>
                  <th>Url</th>
                  <th>Paramètre</th>
                  <th>Retour</th>
              </tr>
              <tr class="active">
                    <td>Subject creation</td>
                    <td>POST</td>
                    <td>/api/subjects</td>
                    <td>subject (String) number (int) travel (int : travelId)</td>
                    <td>Code 200: subject (Subject)</td>
                </tr>
                <tr class="danger">
                    <td>Subject list</td>
                    <td>GET</td>
                    <td>/api/subjects</td>
                    <td>None</td>
                    <td>Code 200: subjects (list Subject)</td>
                </tr>
                <tr class="success">
                    <td>Subject select</td>
                    <td>GET</td>
                    <td>/api/subjects/[subjectId}</td>
                    <td>subjectlId (int : subjectId)</td>
                    <td>Code 200: subject (Subject)</td>
                </tr>
                <tr class="info">
                    <td>Subject list by Travel</td>
                    <td>GET</td>
                    <td>/api/travels/{travelId}subjects/</td>
                    <td>travelId (int : travelId)</td>
                    <td>Code 200: subjects (list Subject)</td>
                </tr>
              </table>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
          </div>
        </div><!-- /.modal-content -->
      </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
    <section id="header">
        <img style="width:auto;height: 150px; float:left;"src="{{ asset('bundles/subwaybuddyuser/images/subwwaybuddy.png') }}"/>
        <div class="container">
            <header>
                <nav class="navbar navbar-default" style="height: 51px;">
                  <div class="container-fluid">
                    <!-- Brand and toggle get grouped for better mobile display -->
                    <div class="navbar-header">
                      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                      </button>
                      <a class="navbar-brand" href="#">SubwayBuddy</a>
                    </div>

                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                      <ul class="nav navbar-nav">
                        <li class="active" data-toggle="modal" data-target="#userModal"><a href="#">User <span class="sr-only">(current)</span></a></li>
                        <li><a href="#" data-toggle="modal" data-target="#travelModal">Travel</a></li>
                        <li class="active" data-toggle="modal" data-target="#subjectModal"><a href="#">Subject <span class="sr-only">(current)</span></a></li>
                      </ul>
                    </div><!-- /.navbar-collapse -->
                  </div><!-- /.container-fluid -->
                </nav>
            </header>
        </div>
        <div id="layer"></div>
    </section>

</div>
{% endblock %}